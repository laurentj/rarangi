<?xml version="1.0" encoding="UTF-8"?>
<dao xmlns="http://jelix.org/ns/dao/1.0">
    <datasources>
        <primarytable name="methods_authors" realname="methods_authors" primarykey="name,class_id,author_id" />
    </datasources>
    <record>
        <property name="name" fieldname="name" datatype="string" required="true" maxlength="150"/>
        <property name="class_id" fieldname="class_id" datatype="int" required="true"/>
        <property name="author_id" fieldname="author_id" datatype="int" required="true"/>
        <property name="as_contributor" fieldname="as_contributor" datatype="int" required="true" default="0"/>
    </record>
</dao>
